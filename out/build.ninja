rule gn
  command = /home/larry/Desktop/gn/gn --root=/home/larry/Documents/projects/cpp/cppdemo -q gen .
  description = Regenerating ninja files

build build.ninja: gn
  generator = 1
  depfile = build.ninja.d

subninja toolchain.ninja

build cppdemo: phony obj/cppdemo.stamp
build client$:client: phony client
build $:cppdemo: phony obj/cppdemo.stamp
build server$:server: phony server

build all: phony $
    client $
    obj/cppdemo.stamp $
    server

default all
